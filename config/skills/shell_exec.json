{
  "id": "shell_exec2",
  "name": "Shell Execution v2",
  "description": "Standard tools for file processing.",
  "patterns": [
    {
      "task": "Enumerate files",
      "recommended": "find . -type f",
      "output_type": "List"
    },
    {
      "task": "Enumerate files has the extention",
      "recommended": "find . -type f -name *.py",
      "output_type": "List"
    },
    {
      "task": "Sort numerically (Smallest top)",
      "recommended": "sort -n",
      "output_type": "List"
    },
    {
      "task": "Display content",
      "recommended": "cat \"$1\"",
      "input_type": "Path",
      "output_type": "List"
    },
    {
      "task": "Calculate size",
      "recommended": "du -b \"$1\"",
      "input_type": "Path",
      "output_type": "Single"
    },
    {
      "task": "Select first item",
      "recommended": "head -n 1",
      "output_type": "Single"
    }
  ]
}
